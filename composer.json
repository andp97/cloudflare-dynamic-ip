{
    "name": "adnp97/cloudflare-dynamic-ip",
    "description": "Cloudflare DDNS script for Raspberry Pi 4 or RHEL/Debian/Ubuntu",
    "keywords": ["dynamic-ip", "ddns", "cloudflare"],
    "homepage": "https://github.com/andp97/cloudflare-dynamic-ip",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/andp97/cloudflare-dynamic-ip/issues",
        "source": "https://github.com/andp97/cloudflare-dynamic-ip"
    },
    "authors": [
        {
            "name": "Andrea Pavone",
            "email": "info@andreapavone.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "cloudflare/sdk": "^1.3",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^10.0",
        "illuminate/view": "^10.0",
        "laminas/laminas-text": "^2.10",
        "laravel-zero/framework": "^10.0",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["cloudflare-ddns"]
}
